                            android:fontFamily="@font/hi_maum_r"
                            android:text="@string/text_sugar_before"
                            android:textSize="@dimen/_8_dp"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/hi_maum_r"
                            android:text="90mg/dl 이하"
                            android:textSize="@dimen/_8_dp"/>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="left">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="@dimen/_3_dp"
                            android:drawablePadding="5dp"
                            android:fontFamily="@font/hi_maum_r"
                            android:text="@string/text_sugar_after"
                            android:textSize="@dimen/_8_dp"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="@dimen/_10_dp"
                            android:fontFamily="@font/hi_maum_r"
                            android:text="120mg/dl 이하"
                            android:textSize="@dimen/_8_dp"/>
                    </LinearLayout>


                </LinearLayout>

                <LinearLayout
