                android:checked="true"
                android:fontFamily="@font/hi_maum_r"
                android:text="@string/text_graph"
                android:textSize="@dimen/px_46"
                android:textStyle="bold"/>

            <RadioButton
                android:id="@+id/btn_sugar_history"
                style="@style/pink_radio_style_right"
                android:layout_width="@dimen/_140_dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/_40_dp"
                android:layout_marginRight="@dimen/_40_dp"
                android:layout_weight="1"
                android:textColor="@color/mother_btn_color"
                android:fontFamily="@font/hi_maum_r"
                android:text="@string/text_history"
                android:textSize="@dimen/px_46"
                android:textStyle="bold"/>

            <!---->
        </RadioGroup>


    </LinearLayout>

    <LinearLayout
        android:id="@+id/layout_sugar_history"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/colorWhite"
        android:orientation="vertical"
        android:visibility="gone">


