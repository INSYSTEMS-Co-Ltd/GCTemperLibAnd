                        android:layout_height="wrap_content"
                        android:background="@drawable/background_fever_map_bottom"
                        android:padding="10dp">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Slide를 내리면 열지도를 크게 볼 수 있어요."/>

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/arrow_right"
                            android:layout_alignParentRight="true"
                            android:rotation="90"/>

                    </RelativeLayout>
                    <LinearLayout
                        android:id="@+id/slide_menu_layout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentBottom="true"
                        android:background="@color/colorWhite"
                        android:clickable="true"
                        android:visibility="gone"
                        tools:visibility="visible"
                        android:orientation="vertical">
                        <View
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/dp_2"
                            android:layout_alignParentRight="true"
                            android:background="@color/colorGray"/>

                        <LinearLayout
                            android:id="@+id/fever_map_menu_1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:padding="10dp">
                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="체온관리"
                                android:drawableLeft="@drawable/commu_btn_6ov"
                                android:textColor="@color/colorBlack"
                                android:drawablePadding="20dp"/>

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:src="@drawable/arrow_right" />
                        </LinearLayout>
                        <View
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/dp_2"
                            android:layout_alignParentRight="true"
                            android:background="@color/colorGray"/>

                        <LinearLayout
                            android:id="@+id/fever_map_menu_2"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:padding="10dp">

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="검강검진 예약"
                                android:drawableLeft="@drawable/commu_btn_6ov"
                                android:textColor="@color/colorBlack"
                                android:drawablePadding="20dp"/>

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:src="@drawable/arrow_right" />
                        </LinearLayout>
                        <View
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/dp_2"
                            android:layout_alignParentRight="true"
                            android:background="@color/colorGray"/>

                        <LinearLayout
                            android:id="@+id/fever_map_menu_3"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:padding="10dp">

                            <TextView
