<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:ignore="missingPrefix"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <RelativeLayout
        android:id="@+id/group_child_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/color_dadfe1"
        android:padding="@dimen/_19_dp">

        <TextView
            android:id="@+id/group_child_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/empty_data"
            android:textColor="@color/color_333333"
            android:textSize="@dimen/_15_dp"
            android:lineSpacingExtra="@dimen/_5_dp"
            android:fontFamily="@font/hi_gothic_m"/>
    </RelativeLayout>

</LinearLayout>