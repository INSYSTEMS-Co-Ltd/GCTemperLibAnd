<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/root_view"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:ignore="missingPrefix">

    <include android:id="@+id/common_back_top" layout="@layout/common_back_top" />

    <!-- 백그라운드 -->
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/common_back_top"
        android:background="@color/color_100ffffff">
        <!-- 포그라운드 -->
        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:overScrollMode="never">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <!-- 사전 체크 문항 설정 -->
                <RelativeLayout
                    android:id="@+id/MonJin_layout"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_53_dp"
                    android:layout_marginTop="@dimen/_10_dp"
                    android:paddingLeft="@dimen/_19_dp"
                    android:paddingRight="@dimen/_19_dp">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="@string/setting_monjin"
                        android:textColor="@color/txt_dark_bold"
                        android:textSize="@dimen/_15_dp"
                        android:fontFamily="@font/hi_gothic_b" />

                    <ImageView
                        android:id="@+id/MonJin_img"
                        android:layout_width="@dimen/_8_dp"
                        android:layout_height="@dimen/_12_dp"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:src="@drawable/btn_faq_answer" />

                </RelativeLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_1_dp"
                    android:background="@color/bg_gray"
                    android:layout_marginLeft="@dimen/_19_dp"
                    android:layout_marginRight="@dimen/_19_dp"/>




                <!-- 준회원 정보 변경 설정 -->
                <RelativeLayout
                    android:id="@+id/junmember_layout"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_53_dp"
                    android:layout_marginTop="@dimen/_10_dp"
                    android:paddingLeft="@dimen/_19_dp"
                    android:paddingRight="@dimen/_19_dp">

                    <TextView
                        android:id="@+id/junmember_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="@string/setting_junmember"
                        android:textColor="@color/txt_dark_bold"
                        android:textSize="@dimen/_15_dp"
                        android:fontFamily="@font/hi_gothic_b" />

                    <ImageView
                        android:layout_width="@dimen/_8_dp"
                        android:layout_height="@dimen/_12_dp"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:src="@drawable/btn_faq_answer" />

                </RelativeLayout>

                <View
                    android:id="@+id/jun_line"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_1_dp"
                    android:background="@color/bg_gray"
                    android:layout_marginLeft="@dimen/_19_dp"
                    android:layout_marginRight="@dimen/_19_dp"/>
                <!-- 정회원 정보 변경 설정 -->
                <RelativeLayout
                    android:id="@+id/memberswitch_layout"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_53_dp"
                    android:layout_marginTop="@dimen/_10_dp"
                    android:paddingLeft="@dimen/_19_dp"
                    android:paddingRight="@dimen/_19_dp">

                    <TextView
                        android:id="@+id/memberswitch_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="@string/setting_meberswitch"
                        android:textColor="@color/txt_dark_bold"
                        android:textSize="@dimen/_15_dp"
                        android:fontFamily="@font/hi_gothic_b" />

                    <ImageView
                        android:layout_width="@dimen/_8_dp"
                        android:layout_height="@dimen/_12_dp"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:src="@drawable/btn_faq_answer" />

                </RelativeLayout>

                <View
                    android:id="@+id/mem_line"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_1_dp"
                    android:background="@color/bg_gray"
                    android:layout_marginLeft="@dimen/_19_dp"
                    android:layout_marginRight="@dimen/_19_dp"/>
                <!-- 알림 설정 -->
                <RelativeLayout
                    android:id="@+id/alarm_layout"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_53_dp"
                    android:layout_marginTop="@dimen/_10_dp"
                    android:paddingLeft="@dimen/_19_dp"
                    android:paddingRight="@dimen/_19_dp">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="@string/setting_alarm"
                        android:textColor="@color/txt_dark_bold"
                        android:textSize="@dimen/_15_dp"
                        android:fontFamily="@font/hi_gothic_b" />

                    <ImageView
                        android:id="@+id/alarm_img"
                        android:layout_width="@dimen/_8_dp"
                        android:layout_height="@dimen/_12_dp"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:src="@drawable/btn_faq_answer" />

                </RelativeLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_1_dp"
                    android:background="@color/bg_gray"
                    android:layout_marginLeft="@dimen/_19_dp"
                    android:layout_marginRight="@dimen/_19_dp"/>


                <!-- 인증 관리 -->
                <RelativeLayout
                    android:id="@+id/auth_layout"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_53_dp"
                    android:layout_marginTop="@dimen/_10_dp"
                    android:paddingLeft="@dimen/_19_dp"
                    android:paddingRight="@dimen/_19_dp">

                    <TextView
                        android:id="@+id/auth_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="@string/login_info"
                        android:textColor="@color/txt_dark_bold"
                        android:textSize="@dimen/_15_dp"
                        android:fontFamily="@font/hi_gothic_b"/>

                    <ImageView
                        android:id="@+id/auth_img"
                        android:layout_width="@dimen/_8_dp"
                        android:layout_height="@dimen/_12_dp"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:src="@drawable/btn_faq_answer" />
                </RelativeLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_1_dp"
                    android:background="@color/bg_gray"
                    android:layout_marginLeft="@dimen/_19_dp"
                    android:layout_marginRight="@dimen/_19_dp"/>

                <!-- 서비스 소개 -->
                <RelativeLayout
                    android:id="@+id/about_service_layout"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_53_dp"
                    android:layout_marginTop="@dimen/_15_dp"
                    android:paddingLeft="@dimen/_19_dp"
                    android:paddingRight="@dimen/_19_dp">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="@string/about_service"
                        android:textColor="@color/txt_dark_bold"
                        android:textSize="@dimen/_15_dp"
                        android:fontFamily="@font/hi_gothic_b" />

                    <ImageView
                        android:id="@+id/about_service_img"
                        android:layout_width="@dimen/_8_dp"
                        android:layout_height="@dimen/_12_dp"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:src="@drawable/btn_faq_answer" />

                </RelativeLayout>

                <!-- 서비스 소개 -->
                <RelativeLayout
                    android:id="@+id/personal_terms_1_layout"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_53_dp"
                    android:paddingLeft="@dimen/_19_dp"
                    android:paddingRight="@dimen/_19_dp">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="@string/setting_personal_terms"
                        android:textColor="@color/txt_dark_bold"
                        android:textSize="@dimen/_15_dp"
                        android:fontFamily="@font/hi_gothic_b" />

                    <ImageView
                        android:id="@+id/personal_terms_1_img"
                        android:layout_width="@dimen/_8_dp"
                        android:layout_height="@dimen/_12_dp"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:src="@drawable/btn_faq_answer" />

                </RelativeLayout>

                <!-- 서비스 소개 -->
                <RelativeLayout
                    android:id="@+id/personal_terms_2_layout"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_53_dp"
                    android:paddingLeft="@dimen/_19_dp"
                    android:paddingRight="@dimen/_19_dp">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="@string/setting_martketing_terms"
                        android:textColor="@color/txt_dark_bold"
                        android:textSize="@dimen/_15_dp"
                        android:fontFamily="@font/hi_gothic_b" />

                    <Switch
                        android:id="@+id/push_alarm_cb_1"
                        android:layout_width="50dp"
                        android:layout_height="25dp"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"/>

                </RelativeLayout>

                <!-- 도움말 -->
                <RelativeLayout
                    android:id="@+id/location_terms_layout"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_53_dp"
                    android:paddingLeft="@dimen/_19_dp"
                    android:paddingRight="@dimen/_19_dp">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="@string/setting_location_terms"
                        android:textColor="@color/txt_dark_bold"
                        android:textSize="@dimen/_15_dp"
                        android:fontFamily="@font/hi_gothic_b" />

                    <Switch
                        android:id="@+id/push_alarm_cb_2"
                        android:layout_width="50dp"
                        android:layout_height="25dp"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/personal_terms_3_layout"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_53_dp"
                    android:paddingLeft="@dimen/_19_dp"
                    android:paddingRight="@dimen/_19_dp"
                    android:visibility="gone">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="@string/personal_terms_3"
                        android:textColor="@color/txt_dark_bold"
                        android:textSize="@dimen/_15_dp"
                        android:fontFamily="@font/hi_gothic_b" />

                    <ImageView
                        android:id="@+id/personal_terms_3_img"
                        android:layout_width="@dimen/_8_dp"
                        android:layout_height="@dimen/_12_dp"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:src="@drawable/btn_faq_answer" />
                </RelativeLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_1_dp"
                    android:background="@color/bg_gray"
                    android:layout_marginTop="@dimen/_15_dp"
                    android:layout_marginLeft="@dimen/_19_dp"
                    android:layout_marginRight="@dimen/_19_dp"/>

                <!-- 버전정보 -->
                <RelativeLayout
                    android:id="@+id/version_info_layout"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_53_dp"
                    android:paddingLeft="@dimen/_19_dp"
                    android:paddingRight="@dimen/_19_dp">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="@string/version_info"
                        android:textColor="@color/txt_dark_bold"
                        android:textSize="@dimen/_15_dp"
                        android:fontFamily="@font/hi_gothic_b"/>

                    <ImageView
                        android:id="@+id/version_info_img"
                        android:layout_width="@dimen/_8_dp"
                        android:layout_height="@dimen/_12_dp"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:src="@drawable/btn_faq_answer" />

                    <TextView
                        android:id="@+id/version_info_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="@dimen/_15_dp"
                        android:layout_toLeftOf="@+id/version_info_img"
                        android:text="1.0"
                        android:textColor="@color/txt_dark_lithg"
                        android:textSize="@dimen/_13_dp"
                        android:visibility="visible"
                        android:fontFamily="@font/hi_gothic_m"/>

                    <ImageView
                        android:id="@+id/version_info_new_img"
                        android:layout_width="@dimen/_19_dp"
                        android:layout_height="@dimen/_19_dp"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="@dimen/_2_5_dp"
                        android:layout_toLeftOf="@+id/version_info_tv"
                        android:src="@drawable/icon_new_01"
                        android:visibility="gone" />
                </RelativeLayout>


                <!-- 로그아웃 -->
                <RelativeLayout
                    android:id="@+id/logout_layout"
                    android:layout_width="match_parent"
                    android:clickable="true"
                    android:layout_height="@dimen/_53_dp"
                    android:paddingLeft="@dimen/_19_dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="40dp"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:paddingRight="@dimen/_19_dp">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingTop="15dp"
                        android:paddingBottom="15dp"
                        android:background="@drawable/background_5_506bb0d7"
                        android:clickable="false"
                        android:gravity="center"
                        android:layout_centerVertical="true"
                        android:text="@string/txt_logout"
                        android:textColor="@color/color_ffffff"
                        android:textSize="@dimen/_15_dp"

                        android:fontFamily="@font/hi_gothic_b"/>

                </RelativeLayout>


            </LinearLayout>
        </ScrollView>
    </RelativeLayout>
</RelativeLayout>